    },
    "dependencies": {
        "@material-ui/core": "^4.12.3",
        "@material-ui/icons": "^4.11.2",
        "@react-css/grid": "^1.4.4",
        "clsx": "^1.1.1",
        "next": "11.1.2",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "rebass": "^4.0.7"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.15.4",
        "@babel/preset-react": "^7.14.5",
        "@trivago/prettier-plugin-sort-imports": "^2.0.4",
        "@types/chai": "^4.2.21",
        "@types/mocha": "^9.0.0",
        "@types/react": "17.0.19",
        "@types/react-dom": "^17.0.9",
        "@types/rebass": "^4.0.9",
        "babel-plugin-styled-components": "^1.13.2",
        "chai": "^4.3.4",
        "eslint": "7.32.0",
        "eslint-config-next": "11.1.2",
        "mocha": "^9.1.1",
        "nws": "^1.1.1",
